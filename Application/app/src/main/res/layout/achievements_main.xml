<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".AchievementsActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/tomenu"
        android:background="@drawable/smartback2"
        android:onClick="toMenu"
        app:layout_aspectRatio="100%"
        app:layout_heightPercent="10%"
        app:layout_marginLeftPercent="5%"
        app:layout_marginTopPercent="2.5%" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/reload"
        android:layout_alignParentEnd="true"
        android:background="@drawable/smartbonus"
        android:onClick="toBonus"
        app:layout_aspectRatio="100%"
        app:layout_heightPercent="10%"
        app:layout_marginRightPercent="5%"
        app:layout_marginTopPercent="2.5%" />

    <androidx.legacy.widget.Space
        android:id="@+id/s"
        android:layout_width="match_parent"
        android:layout_below="@id/reload"
        app:layout_heightPercent="2%" />

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_below="@id/s"
        app:layout_heightPercent="100%">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/trashb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/trashs"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/trashg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/mistakeb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/mistakes"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/mistakeg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/paperb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/papers"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/paperg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/metalb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/metals"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/metalg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/organicb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/organics"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/organicg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/notrecycleb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/notrecycles"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/notrecycleg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/glassb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/glasss"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/glassg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/plasticb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/plastics"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/plasticg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/manb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/mans"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/mang"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/carb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/cars"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/carg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/robotb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/robots"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/robotg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/factoryb"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/factorys"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />

                <ImageView
                    android:id="@+id/factoryg"
                    android:layout_width="100dp"
                    android:layout_height="150dp"
                    android:layout_weight="1"
                    android:padding="10dp"
                    app:srcCompat="@drawable/empty" />
            </LinearLayout>

        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.percentlayout.widget.PercentRelativeLayout>